{
  "version": 2,
  "builds": [
    {
      "src": "Images/**",
      "use": "@vercel/static"
    },
    {
      "src": "Songs/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Images/(.*)",
      "dest": "/Images/$1"
    },
    {
      "src": "/Songs/(.*)",
      "dest": "/Songs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
