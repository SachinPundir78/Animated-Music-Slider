{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "Images/**",
      "use": "@vercel/static"
    },
    {
      "src": "Songs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.html" }
     { "src": "/Images/(.*)", "dest": "/Images/$1" },
     { "src": "/Songs/(.*)", "dest": "/Songs/$1" },
  ]
}
